@model IEnumerable<ApplicationUser>
@{

    ViewData["Title"] = "Üyeler";
}



><table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">İsim</th>
            <th scope="col">Cinsiyet</th>
            <th scope="col">Doğum Tarihi</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Name</td>
                <td>@item.Gender</td>
                <td>@item.BirthDate</td>
            </tr>
        }

    </tbody>



</table>
